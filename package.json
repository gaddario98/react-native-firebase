    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "@invertase/react-native-apple-authentication": {
      "optional": true
    },
    "@react-native-google-signin/google-signin": {
      "optional": true
    },
    "react-apple-signin-auth": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "lib",
    "index.ts"
